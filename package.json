{
  "name": "@chordbook/charts",
  "version": "0.0.2",
  "description": "SVG chord diagrams for Guitar / Ukulele",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite",
    "prepare": "tsup src/index.ts --format esm,cjs --minify --dts --sourcemap",
    "build": "vite build --outDir demo --sourcemap",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chordbook/charts.git"
  },
  "keywords": [
    "guitar",
    "music",
    "chords"
  ],
  "contributors": [
    "Brandon Keepers",
    "Mohit Cheppudira"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chordbook/charts/issues"
  },
  "homepage": "https://github.com/chordbook/charts#readme",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/zepto": "^1.0.36",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.2.4"
  }
}
